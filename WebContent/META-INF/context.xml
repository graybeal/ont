<?xml version="1.0" encoding="UTF-8"?>
<Context>
	<Resource name="@bioportal.datasource.name@"
		type="javax.sql.DataSource" 
		driverClassName="@bioportal.jdbc.driver@"
		username="@bioportal.jdbc.username@" 
		password="@bioportal.jdbc.password@"
		url="@bioportal.jdbc.url@" 
		maxActive="@bioportal.jdbc.maxactive@"
		maxIdle="@bioportal.jdbc.maxidle@" 
		maxWait="@bioportal.jdbc.maxwait@" />
</Context>
